<h1>Create New Employee</h1>

<% if flash[:messages] %>
  <%= flash[:messages] %>
<% end %>
</br>
</br>

<%= form_for @employee do |f| %>
  <%= f.label :first_name %>
  <%= f.text_field :first_name %></br>
  <%= f.label :last_name %>
  <%= f.text_field :last_name %></br>
  <%= f.label :alias %>
  <%= f.text_field :alias %></br>
  <%= f.label :title %>
  <%= f.text_field :title %></br>
  <%= f.label :office %>
  <%= f.text_field :office %></br>
  <%= f.label :dog %>
  <%= f.collection_select :dog_id, @dogs, :id, :name %></br>
  <%= f.label "Image URL" %>
  <%= f.text_field :img_url %></br>
  <%= f.submit :submit %>
<% end %>