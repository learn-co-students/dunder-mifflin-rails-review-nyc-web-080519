<h1><%= @dog.name %></h1>
<ul>
    <li>Breed: <%= @dog.breed %></li>
    <li>Age: <%= @dog.age %></li>
    <li>Employees: 
        <ul>
        <% @dog_employees.each do |employee| %>
            <li><%= link_to employee.first_name + " " + employee.last_name, employee_path(employee) %></li>
        <% end %>
        </ul>
    </li>
</ul>